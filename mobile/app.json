{
  "expo": {
    "name": "Cerrhud Lab",
    "slug": "cerrhud-lab",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icons/icon.png",
    "scheme": "cerrhudlab",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zase.cerrhudlab",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.zase.cerrhudlab",
      "adaptiveIcon": {
        "foregroundImage": "assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/favicon/favicon.ico"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/poppins/400Regular/Poppins_400Regular.ttf",
            "node_modules/@expo-google-fonts/roboto-serif/700Bold/RobotoSerif_700Bold.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#d9d5e8",
          "image": "./assets/icons/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": [
              "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
            ]
          }
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      "@maplibre/maplibre-react-native",
      "expo-mail-composer"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1cd7dab8-77d5-49cf-bb11-969f49491581"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
